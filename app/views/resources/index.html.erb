<h1>Resources#index</h1>
<p>Find me in app/views/resources/index.html.erb</p>

<%= form_with model: @resource   do |form| %>
      <div class="form-group">
          <%= form.label :input, class: "col-md-4 control-label" %>
          <%= form.text_field :input, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= form.label :title, class: "col-md-4 control-label" %>
          <%= form.text_field :title, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= form.label :description, class: "col-md-4 control-label" %>
          <%= form.text_field :description, class: "form-control" %>
      </div>
     <div class="form-group">
        <%= form.submit%>
     </div>
<%end%>
<%= form_tag search_resource_path, :method => :get do %>

  <%= text_field_tag :query, params[:query] %> <%= submit_tag "Search!" %>

<% end %>